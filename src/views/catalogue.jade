extends ./base.jade
block append style
    link(rel="stylesheet", type="text/css", href="/assets/style.css")
block body
  div#container
    div#binding &nbsp;
    div#page &nbsp;
      a#logOut(href="/logout") Log out
      div#birdList
        if birds || birds.length > 0
          each val in birds
            div.birdBox
              h3 #{val.name}
        a#newBird(href="/maker") &nbsp;New
block scripts
        script(src="/assets/catalogue.js")